{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "name": "properties",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "category": {
      "model": "categories",
      "via": "properties"
    },
    "price": {
      "type": "biginteger",
      "required": false
    },
    "bedrooms": {
      "type": "string"
    },
    "bathroom": {
      "type": "string"
    },
    "sittingroom": {
      "type": "string"
    },
    "favourites": {
      "collection": "favourite"
    },
    "reviews": {
      "collection": "review"
    },
    "rating": {
      "type": "decimal"
    },
    "users_permissions_user": {
      "via": "properties",
      "plugin": "users-permissions",
      "model": "user"
    },
    "featured": {
      "type": "boolean"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "features": {
      "type": "component",
      "repeatable": true,
      "component": "features.features"
    },
    "state": {
      "type": "string",
      "required": true
    },
    "per": {
      "type": "enumeration",
      "enum": [
        "day",
        "week",
        "month",
        "threeMonths",
        "sixMonths",
        "year",
        "twoYears",
        "threeYears",
        "fourYears",
        "fiveYears"
      ],
      "required": false
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Apartment",
        "Duplex",
        "MiniFlat",
        "Bungalow",
        "House",
        "Mansion",
        "Villa",
        "Land"
      ]
    },
    "size": {
      "type": "decimal"
    },
    "description": {
      "type": "richtext"
    }
  }
}
